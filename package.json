{
    "name": "toml",
    "displayName": "TOML Language Support",
    "description": "TOML language support",
    "version": "0.6.0",
    "publisher": "be5invis",
    "repository": {
        "type": "git",
        "url": "https://github.com/GalAster/vscode-toml.git"
    },
    "engines": {
        "vscode": "^1.8.0"
    },
    "activationEvents": [
        "onLanguage:toml"
    ],
    "categories": [
        "Programming Languages",
        "Formatters"
    ],
    "scripts": {
        "build": "ts-node source/build.ts && tsc",
        "lint": "tslint **/*.ts --fix"
    },
    "contributes": {
        "languages": [
            {
                "id": "toml",
                "aliases": [
                    "TOML"
                ],
                "extensions": [
                    ".toml",
                    ".tml"
                ],
                "filenames": [
                    "Cargo.lock",
                    "Gopkg.lock"
                ],
                "mimetypes": [
                    "text/x-toml"
                ],
                "configuration": "./extension/toml.configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "toml",
                "scopeName": "source.toml",
                "path": "./extension/toml.tmLanguage.json"
            }
        ]
    },
    "dependencies": {
        "toml_vscode": "^0.1.3"
    },
    "devDependencies": {
        "@types/node": "^13.7.6",
        "vscode": "^1.1.37"
    }
}
